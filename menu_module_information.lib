# |       Function f_main_information      |
# +----------------------------------------+
#
#  Inputs: None.
#    Uses: ARRAY_FILE, GENERATED_FILE, MENU_TITLE.
# Outputs: None.
#
f_main_information () { # Create and display the Menu.
      . $THIS_DIR/lib_cli-common.lib    # invoke module/library.
      #
      # Create the arrays for the menu options and display the Menu.
      ARRAY_FILE="$THIS_DIR/menu_module_information.lib"
      GENERATED_FILE="cliappmenu_generated.tmp"
      MENU_TITLE="Information_Menu"  # Menu title must substitute underscores for spaces
      f_menu_setup $ARRAY_FILE $GENERATED_FILE $MENU_TITLE
} # End of function f_main_information.
